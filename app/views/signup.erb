    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script src="js/bootstrap.js"></script>


    <!-- this section contains the navigation bar code -->

    <nav class="py-2 bg-light border-bottom" >
        <div class="container d-flex flex-wrap">

          <ul style="font-family:fantasy ;" class="nav me-auto">
          <a  class="navbar-brand" class="nav-link link-dark px-2 active" href="#">wakanda</a>
          </ul>
     

        
          <ul class="nav" >
            <li class="nav-item"><a href="loginin.html" class="nav-link link-dark px-2">Login</a></li>
            <li class="nav-item"><a href="signup.html" class="nav-link link-dark px-2">Sign up</a></li>
          </ul>
        </div>
      </nav>
<br>
      <!-- the secton contains the Sign-up box -->




      <!-- the secton contains the leftside Sign-up box -->

      <div class="row"  class="container-fluid" >
        <div class="col-4" >
        <img  src="images/africanface.png" alt="Cinque Terre"  >
        </div>


          <!-- the secton contains the centerside Sign-up box -->

        <div class="col-4">
        <form action="stage1" method="get">
          <div  style="font-family:Verdana, Geneva, Tahoma, sans-serif ; font-size: 20px;"><caption >Welcome, let's Start UP by getting you Signed UP</caption></div>
          <br>

          <div>
            <div><label for="email" class="form-label" >Email</label></div>
            <input type="email" id="email" name="email" class="form-control" placeholder="example@email.com" required>
          </div><br>

          <div>
            <div><label for="uname" class="form-label" >Username</label></div>
            <input type="text" id="signup_uname" name="signup_uname" class="form-control" placeholder="username" required>
          </div><br>

          <div>
            <div><label for="ps" class="form-label" >Password</label></div>
            <input type="password" id="signup_ps" name="signup_ps" class="form-control" maxlength="10" placeholder="password" required>
          </div><br>

          <div>
            <button  class="btn btn-secondary" type="submit">submit</button>
          </div>
        </form>
      </div>
        



        <!-- the secton contains the rightside Sign-up box -->

        <div class="col-4" > </div>
      </div>


</body>
